{
  "name": "train-puzzle",
  "version": "1.0.0",
  "description": "A project using Pug and Sass with src/public folders",
  "main": "src/script.js",
  "scripts": {
    "pug": "pug src/index.pug --out public/",
    "sass": "sass src/style.scss public/style.css",
    "copy:js": "copy src\\script.js public\\script.js",
    "build": "npm run pug && npm run sass && npm run copy:js",
    "watch:pug": "pug src/index.pug --out public/ --watch",
    "watch:sass": "sass --watch src/style.scss:public/style.css",
    "watch:js": "nodemon --watch src/script.js --exec \"npm run copy:js\"",
    "watch": "concurrently \"npm run watch:pug\" \"npm run watch:sass\" \"npm run watch:js\"",
    "clean": "rimraf public/*"
  },
